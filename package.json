    "@payloadcms/payload-cloud": "3.9.0",
    "@payloadcms/richtext-lexical": "3.9.0",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "gsap": "^3.12.5",
    "katex": "^0.16.21",
    "lenis": "^1.1.18",
